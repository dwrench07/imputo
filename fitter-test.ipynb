   "source": [
    "from scipy import stats\n",
    "from fitter import Fitter\n",
    "import numpy\n",
    "\n",
    "\n",
    "data = stats.gamma.rvs(2, loc=1.5, scale=2, size=100000)\n",
    "\n",
    "f = Fitter(data, distributions=['gamma'])\n",
    "f.fit()\n",
    "f.summary()"
   ]
